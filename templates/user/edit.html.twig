<!DOCTYPE html>
<html>

<head>
  <meta charset="UTF-8">
  <title>Title</title>
  <script src="./js/vue.js"></script>
  <!-- 引入样式 -->
  <link rel="stylesheet" href="https://unpkg.com/element-ui/lib/theme-chalk/index.css">
  <!-- 引入组件库 -->
  <script src="https://unpkg.com/element-ui/lib/index.js"></script>
  <style>
    html,
    body {
      padding: 0;
      margin: 0;
    }

    .top {
      width: 100%;
      background: red;
      height: 300px;
      overflow: hidden;
      position: relative;
    }

    .top .top-bg {
      width: 100%;
      height: 300px;
    }

    .data-container {
      width: 600px;
      height: 80px;
      display: flex;
      align-items: center;
      justify-content: space-around;
      position: absolute;
      right: 0;
      top: 0
    }

    .data-container .el-button {
      width: 100px;
      height: 40px;
      border-radius: 20px;
      border-color: #fcfdfb;
      background: #fcfdfb;
      display: flex;
      align-items: center;
      padding: 0;
      justify-content: space-around;
    }

    .bottom {
      background: rgb(246, 246, 246);
      padding-top: 30px;
      height: 1300px;
      padding-left: 10%;
    }

    .left {
      margin: 0 50px;
      float: left;
      width: 300px;
      height: 800px;
      border-radius: 40px;
      border: 1px solid rgb(216, 216, 216);
      text-align: center;
      padding: 40px 20px;
    }

    .left-img-container {
      display: inline-block;
      width: 180px;
      height: 180px;
      border-radius: 100px;
      overflow: hidden;
    }

    .left .left-item {
      margin-top: 40px;
      text-align: left;
    }

    .left .left-line {
      margin-top: 40px;
      width: 340px;
      margin-left: -20px;
      height: 1px;
      background: rgb(216, 216, 216);
    }

    .right {
      overflow: hidden;
    }

    .right-item {
      margin-top: 20px;
      margin-left: 60px;
      font-size: 20px;
      overflow: hidden;
    }

    .right-item .item-top {
      font-weight: bold;
      margin-bottom: 10px;
    }

    .right-item .el-input {
      width: 300px;

    }

    .el-input__inner {
      border-color: #cccccc;
    }

    .right-item .line-throw {
      float: left;
      margin-right: 40px;
    }

    .numberSelect {
      display: inline-block;
      height: 40px;
      line-height: 30px;
      color: #aaaaaa;
      margin-right: 5px;
      position: absolute;
      left: 7px;
      z-index: 999;
      border-right: 1px solid #cccccc;
      padding: 5px;
    }

    .numberSelect-container .el-input__inner {
      padding-left: 72px;
    }

    .btn-container {
      width: 50%;
      margin-top: 40px;
      display: flex;
      align-items: center;
      justify-content: space-around;
    }

    .left-img {
      width: 180px;
      height: 180px;
      display: flex;
      border-radius: 50%;
      align-items: center;
      justify-content: center;
      overflow: hidden;
    }
  </style>
</head>

<body>
<div id="app">
  <div class="top">
    <img src="./img/topbanner.jpg" class="top-bg">
    <div class="data-container">
      <div>
        A propos
      </div>
      <div>
        Hebergement
      </div>
      <div>
        Services
      </div>
      <div>
        <el-dropdown>
          <el-button type="primary">
            <i class="el-icon-s-unfold" style="font-size: 30px;margin-right: 5px;color: rgb(71,73,70)"></i><img src="./img/head.jpg" style="height: 25px;width: 25px">
          </el-button>
          <el-dropdown-menu slot="dropdown">
            <el-dropdown-item>Compte</el-dropdown-item>
            <el-dropdown-item>Messages</el-dropdown-item>
            <el-dropdown-item>Voyages</el-dropdown-item>
            <el-dropdown-item>Enregistres</el-dropdown-item>
            <el-dropdown-item>Aide</el-dropdown-item>
            <el-dropdown-item>Deconnection</el-dropdown-item>
          </el-dropdown-menu>
        </el-dropdown>
      </div>
    </div>
  </div>
  <div class="bottom">
    <div class="left">
      <div class="left-img-container">
        <img src="./img/head.jpg" class="left-img">
      </div>
      <div class="left-item" style="text-align: center;margin-top: 20px;text-decoration: underline">
        metter a your ia photo
      </div>
      <div class="left-item" style="font-weight: bold;text-decoration: underline;margin-top: 60px">
        Infos personnells
      </div>
      <div class="left-item">
        metter a your ia photo
      </div>
      <div class="left-item">
        metter a your ia photo
      </div>
      <div class="left-item">
        metter a your ia photo
      </div>
      <div class="left-line">
      </div>
      <div class="left-item" style="font-weight: bold">
        Messages
      </div>
      <div class="left-item" style="font-weight: bold">
        Messages
      </div>
      <div class="left-item" style="font-weight: bold">
        Messages
      </div>
    </div>
    <div class="right">
      <h2>Modifler ie profll!</h2>
      <div style="text-decoration: underline;margin-left: 30px;margin-top: 20px">
        Modifier le profll
      </div>
      <div class="right-container">
        <div class="right-item">
          <div class="line-throw">
            <div class="item-top">Nom</div>
            <el-input v-model="Nom" placeholder="please"></el-input>
          </div>
          <div class="line-throw">
            <div class="item-top">prenom</div>
            <el-input v-model="prenom" placeholder="please"></el-input>
          </div>
        </div>
        <div class="right-item">
          <div class="item-top">sexe</div>
          <el-select v-model="sexe" placeholder="please">
            <el-option v-for="item in options" :key="item.value" :label="item.label" :value="item.value">
            </el-option>
          </el-select>
        </div>
        <div class="right-item">
          <div class="item-top">date</div>
          <div class="line-throw">
            <div class="item-top" style="font-weight: normal">Jour</div>
            <el-select v-model="dateJour" placeholder="please">
              <el-option v-for="item in options" :key="item.value" :label="item.label" :value="item.value">
              </el-option>
            </el-select>
          </div>
          <div class="line-throw">
            <div class="item-top" style="font-weight: normal">Mois</div>
            <el-select v-model="dateMois" placeholder="please">
              <el-option v-for="item in options" :key="item.value" :label="item.label" :value="item.value">
              </el-option>
            </el-select>
          </div>
          <div class="line-throw">
            <div class="item-top" style="font-weight: normal">Annee</div>
            <el-select v-model="dateAnnee" placeholder="please">
              <el-option v-for="item in options" :key="item.value" :label="item.label" :value="item.value">
              </el-option>
            </el-select>
          </div>
        </div>
        <div class="right-item">
          <div class="item-top">email</div>
          <el-input v-model="email" placeholder="please"></el-input>
        </div>
        <div class="right-item">
          <div class="item-top">numberSelect</div>
          <el-select v-model="numberSelect" placeholder="please">
            <el-option v-for="item in options" :key="item.value" :label="item.label" :value="item.value">
            </el-option>
          </el-select>
          <div style="margin-top: 15px;position: relative" class="numberSelect-container">
            <span class="numberSelect">+33</span>
            <el-input v-model="numberValue" placeholder="please"></el-input>
          </div>
        </div>

        <div class="right-item">
          <div class="item-top">Adress</div>
          <div class="item-top" style="font-weight: normal">Pay</div>
          <el-select v-model="AdressePay" placeholder="please">
            <el-option v-for="item in options" :key="item.value" :label="item.label" :value="item.value">
            </el-option>
          </el-select>
        </div>
        <div class="right-item">
          <div class="item-top" style="font-weight: normal">Adr</div>
          <el-input v-model="AdresseAdr" placeholder="please"></el-input>
        </div>
        <div class="right-item">
          <div class="item-top" style="font-weight: normal">Com</div>
          <el-input v-model="AdresseCom" placeholder="please"></el-input>
        </div>
        <div class="right-item">
          <div class="line-throw">
            <div class="item-top" style="font-weight: normal">Code</div>
            <el-input v-model="AdresseCode" placeholder="please"></el-input>
          </div>
          <div class="line-throw">
            <div class="item-top" style="font-weight: normal">Ville</div>
            <el-input v-model="AdresseCode" placeholder="please"></el-input>
          </div>
        </div>
      </div>
      <div class="btn-container">
        <span style="color: red;text-decoration: underline;cursor: pointer">Annuler</span>
        <el-button type="primary">Enregistrer</el-button>
      </div>
      <div style="margin-top: 80px;margin-left: 60px;font-size: 18px">
        @ 2021 RESA,Inc.All rights reserved
      </div>
    </div>
  </div>
</div>

<script>
  new Vue({
    el: '#app',
    data() {
      return {
        options: [{
          value: '+32',
          label: '黄金糕'
        }, {
          value: '+33',
          label: 'France(+33)'
        }, {
          value: '+34',
          label: '蚵仔煎'
        }, {
          value: '+35',
          label: '龙须面'
        }, {
          value: '+36',
          label: '北京烤鸭'
        }],
        Nom: '',
        prenom: '',
        sexe: '',
        dateJour: '',
        dateMois: '',
        dateAnnee: '',
        email: '',
        numberSelect: '+33',
        numberValue: '',
        AdressePay: '',
        AdresseAdr: '',
        AdresseCom: '',
        AdresseCode: '',
        AdresseVille: '',
      }
    }
  })
</script>
</body>

</html>